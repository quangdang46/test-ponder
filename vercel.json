{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/index.ts"
    }
  ],
  "env": {
    "PONDER_RPC_URL_1": "@PONDER_RPC_URL_1"
  }
}
